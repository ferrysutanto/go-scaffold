# UNCOMMENT LINES TO USE
# APP_NAME=go-scaffold
# APP_HOST=localhost
# APP_PORT=8080
# APP_ENV=dev # env: dev, test, stage, prod
# APP_DEBUG=true # APP_DEBUG should be set to false in production

# AWS_REGION=us-east-1
# AWS_ACCOUNT_ID=123456789012

# LAMBDA_COGNITO_PRE_SIGNUP_FUNCTION_NAME=go-scaffold-cognito-pre-signup
# LAMBDA_COGNITO_POST_CONFIRMATION_FUNCTION_NAME=go-scaffold-cognito-post-confirmation
# LAMBDA_COGNITO_POST_AUTHENTICATION_FUNCTION_NAME=go-scaffold-cognito-post-authentication

# DOCKER_COGNITO_PRE_SIGNUP_IMAGE_NAME=go-scaffold-cognito-pre-signup
# DOCKER_COGNITO_POST_CONFIRMATION_IMAGE_NAME=go-scaffold-cognito-post-confirmation
# DOCKER_COGNITO_POST_AUTHENTICATION_IMAGE_NAME=go-scaffold-cognito-post-authentication

# DATABASE SECTION (supported: postgres, dynamodb/ddb)
# DB_DRIVER=postgres

# POSTGRES CONFIG, ONLY IF DB_DRIVER=postgres
# DB_HOST=localhost
# DB_PORT=5432
# DB_USERNAME=postgres
# DB_PASSWORD=postgres
# DB_NAME=postgres
# DB_SSL_MODE=disable

# DB_REPLICA_DRIVER=postgres
# DB_REPLICA_HOST=localhost
# DB_REPLICA_PORT=5432
# DB_REPLICA_USERNAME=postgres
# DB_REPLICA_PASSWORD=postgres
# DB_REPLICA_NAME=postgres
# DB_REPLICA_SSL_MODE=disable

# DYNAMODB CONFIG, ONLY IF DB_DRIVER=dynamodb/ddb
# DDB_HOST=localhost
# DDB_PORT=8000

# CACHE SECTION (supported: redis)
# CACHE_DRIVER=redis
# CACHE_HOST=localhost
# CACHE_PORT=6379
# CACHE_USERNAME=
# CACHE_PASSWORD=
# CACHE_DB=0

# SONARSCAN SECTION
# SONAR_HOST_URL=http://localhost:9000
# SONAR_LOGIN=admin
# SONAR_PASSWORD=admin
# SONAR_PROJECT_KEY=go-scaffold

# TRACING SECTION
# TRACING_ENABLED=true
# TRACING_AGENT_TYPE=jaeger
# TRACING_AGENT_IS_SECURE=false
# TRACING_AGENT_HOST=localhost
# TRACING_AGENT_PORT=4317
# TRACING_SERVICE_NAME=go-scaffold
# TRACING_SAMPLER_TYPE=const
# TRACING_SAMPLER_PARAM=1
